    <class>edu.uoc.pelp.model.vo.admin.PelpActiveSubjects</class>
    <class>edu.uoc.pelp.model.vo.DeliverTestResult</class>
    <class>edu.uoc.pelp.model.vo.admin.PelpLanguages</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/pelp"/>
      <property name="javax.persistence.jdbc.password" value=".pelp."/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="pelp"/>
      <property name="hibernate.cache.provider_class" value="org.hibernate.cache.NoCacheProvider"/>
    </properties>
  </persistence-unit>
