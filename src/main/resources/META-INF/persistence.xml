    <class>edu.uoc.pelp.model.vo.ActivityDO</class>
    <class>edu.uoc.pelp.model.vo.DeliverresultDO</class>
    <class>edu.uoc.pelp.model.vo.PelpMainlabsubjectsDO</class>
    <class>edu.uoc.pelp.model.vo.DeliverDO</class>
    <class>edu.uoc.pelp.model.vo.ActivitytestresultDO</class>
    <class>edu.uoc.pelp.model.vo.PelpAdminsDO</class>
    <class>edu.uoc.pelp.model.vo.PelpActivesubjectsDO</class>
